{
    "_from": "zsh@^6.1.0",
    "_id": "zsh-bin@6.1.0",
    "_spec": "zsh-bin@^6.1.0",
    "name": "zsh-bin",
    "description": "Statically-linked, hermetic, relocatable ZSH",
    "version": "6.1.0",
    "homepage": "https://github.com/romkatv/zsh-bin",
    "bugs": {
        "url": "https://github.com/romkatv/zsh-bin/issues"
    },
    "keywords": [
        "Zshell",
        "zsh"
    ],
    "zsh-data": {
        "plugin-info": {
            "user": "romkatv",
            "plugin": "zsh-bin"
        },
        "zi-ices": {
            "bgn": {
                "requires": "cp;tar;bgn",
                "from": "gh-r",
                "bpick": "*.tar.gz",
                "lucid": "",
                "as": "null",
                "sbin": "bin/zsh*",
                "atclone": "",
                "atpull": "%atclone",
                "nocompile": "",
                "nocompletions": ""
            },
            "default": {
                "requires": "cp",
                "lucid": "",
                "git": "",
                "depth": "1",
                "as": "null",
                "nocompile": "",
                "nocompletions": "",
                "atclone": "./install -e yes -d /usr/local",
                "atpull": "%atclone"
            },
            "rootless": {
                "requires": "cp",
                "lucid": "",
                "git": "",
                "depth": "1",
                "as": "null",
                "nocompile": "",
                "nocompletions": "",
                "atclone": "./install -e no -d ~/.local",
                "atpull": "%atclone"
            }
        }
    }
}
